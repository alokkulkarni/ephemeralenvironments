{
  "name": "@invincible/backstage-plugin-terraform-environments",
  "version": "0.24.0",
  "main": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "start": "backstage-cli package start",
    "build": "backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack",
    "tsc": "tsc --skipLibCheck",
    "tsc:check": "tsc --skipLibCheck --noEmit",
    "build:types": "tsc --emitDeclarationOnly --outDir dist-types",
    "postinstall": "node -e \"try { require('./scripts/add-plugin-to-app.js') } catch(e) { console.log('Skipping postinstall script') }\""
  },
  "dependencies": {
    "@backstage/core-components": "^0.17.1",
    "@backstage/core-plugin-api": "^1.9.3",
    "@backstage/theme": "^0.6.5",
    "@backstage/integration-react": "^1.1.20",
    "@backstage/integration": "^1.9.3",
    "@octokit/rest": "^19.0.13",
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "react-use": "^17.2.4",
    "date-fns": "^2.30.0"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.32.0",
    "@backstage/core-app-api": "^1.16.1",
    "@backstage/dev-utils": "^1.1.9",
    "@backstage/test-utils": "^1.7.7",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "msw": "^1.0.0",
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0",
    "@types/jest": "^26.0.7",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "cross-fetch": "^3.1.5",
    "typescript": "~4.9.5"
  },
  "files": [
    "dist",
    "dist-types",
    "scripts"
  ],
  "backstage": {
    "role": "frontend-plugin",
    "pluginId": "terraform-environments",
    "pluginPackages": {
      "terraform-environments": {
        "root": "src",
        "browser": true
      }
    }
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "package.json"
      ]
    }
  },
  "module": "./dist/index.esm.js"
}
